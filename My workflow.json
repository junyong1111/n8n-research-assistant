    {
      "parameters": {
        "jsCode": "// Sort papers by citations (descending) and get Top 1\nconst papers = $input.item.json.papers || [];\nconst sorted = papers.sort((a, b) => (b.citations || 0) - (a.citations || 0));\nconst seedPaper = sorted[0];\n\nreturn {\n  seed_paper: seedPaper,\n  all_papers: papers\n};"
      },
