    },
    {
      "parameters": {
        "jsCode": "// Flatten citation network into papers array\nconst data = $input.item.json;\nconst allPapers = [];\n\n// Add seed paper\nif (data.seed_paper) {\n  allPapers.push(data.seed_paper);\n}\n\n// Add references\nif (data.references && Array.isArray(data.references)) {\n  allPapers.push(...data.references);\n}\n\n// Add citations\nif (data.citations && Array.isArray(data.citations)) {\n  allPapers.push(...data.citations);\n}\n\nreturn {\n  papers: allPapers,\n  total_papers: allPapers.length\n};"
      },
